@model List<Product>

    <h2>Каталог простой</h2>

        @foreach (var product in Model)
        {
            <h4> @product.Name</h4>
            <p> @product.Id</p>
            <p> @product.Description</p>
            <p> @product.Cost р.</p>
            <p>Размеры: @product.Length x @product.Width x @product.Height см</p>
            <p>Вес: @product.Weight кг</p>
        }
</div>

